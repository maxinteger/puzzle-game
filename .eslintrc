{
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jquery": true
  },
  "globals": {
    "window": false,
    "document": false,
    "navigator": false,
    "alert": false,
    "confirm": false,
    "prompt": false,
    "console": false,
    "angular": false,
    "_": false
  },
  "rules": {
    "quotes": [2, "single" ],
    "no-underscore-dangle": 0,
    "max-statements": [2, 15],
    "max-depth": [1, 2],
    "complexity": [2, 5]
  }
}
